      <Button fx:id="addZoneBtn" layoutX="143.0" layoutY="234.0" mnemonicParsing="false" onAction="#newZoneLine" prefHeight="27.0" prefWidth="79.0" style="-fx-background-color: eb9f65;" text="+ New" textOverrun="CLIP" wrapText="true">
         <font>
            <Font name="Leelawadee UI Bold" size="16.0" />
         </font>
      </Button>
