            <Font name="Leelawadee UI Bold" size="16.0" />
         </font>
      </Button>
      <Button fx:id="trnxBackBtn" layoutX="650.0" layoutY="640.0" mnemonicParsing="false" onAction="#goToMove" prefHeight="30.0" prefWidth="84.0" style="-fx-background-color: #EC6400;" text="Back">
