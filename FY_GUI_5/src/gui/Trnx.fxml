      </Label>
      <TextField fx:id="ccText" layoutX="190.0" layoutY="152.0" prefHeight="25.0" prefWidth="96.0" promptText="Optional" />
      <Label layoutX="468.0" layoutY="147.0" text="PC" textFill="WHITE">
         <font>
            <Font name="Leelawadee UI" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="pcText" layoutX="507.0" layoutY="149.0" prefHeight="25.0" prefWidth="96.0" promptText="Optional" />
      <Button fx:id="addTrnxBtn" layoutX="143.0" layoutY="42.0" mnemonicParsing="false" onAction="#newTrnxLine" prefHeight="27.0" prefWidth="79.0" style="-fx-background-color: eb9f65;" text="+ New" textOverrun="CLIP" wrapText="true">
         <font>
            <Font name="Leelawadee UI Bold" size="16.0" />
         </font>
      </Button>
      <Separator layoutX="76.0" layoutY="196.0" prefHeight="27.0" prefWidth="705.0" />
